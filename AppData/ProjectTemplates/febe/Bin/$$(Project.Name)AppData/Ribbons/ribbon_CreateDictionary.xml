<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ribbon-layout version="23.08.2019">
  <basedir id="pdc-external">$$(Project.Name)AppData\pdcVipIcons</basedir>
  <dictionary file="$$(Project.Name)AppData\Ribbons\ribbonHidden_Dictionary.xml" namespace="hidden" />
  <section id="ribbon.section.hidden" label="Hidden">
    <block>
      <row>
        <cmd 
			cmdstyle="image-and-text-vertical" 
			id="ribbon.cmd.hidden.make-dictionary" 
			label="GenDictionary" 
			menu-label="GenDictionary" 
			run="sleeping" 
		>
          <icon file="$(pdc-external)\_license-cc\actions\address-book-new.ico" />
          <tooltip text="Create and modify Dictionary" />
        </cmd>
      </row>
      <row>
        <cmd 
			cmdstyle="image-and-text-vertical" 
			id="ribbon.cmd.hidden.remove-extra-languages" 
			label="RemoveExtraLngs" 
			menu-label="RemoveExtraLngs" 
			run="sleeping" 
		>
          <icon file="$(pdc-external)\_license-cc\actions\format-remove-node.ico" />
          <tooltip text="Remove Extra Languages from All Dictionaries" />
        </cmd>
      </row>

    </block>
  </section>
</ribbon-layout>